{
	"name": "elake_ai",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"vue:serve": "vue-cli-service serve",
		"vue:build": "vue-cli-service build",
		"electron:serve": "cross-env VUE_DEV_SERVER_URL=http://localhost:8080 electron .",
		"electron:build": "vue-cli-service build && electron-builder install-app-deps && electron-builder"
	},
	"main": "electron/main.js",
	"build": {
		"appId": "top.elake.elake_ai",
		"productName": "elake_ai",
		"directories": {
			"output": "dist_electron"
		},
		"files": [
			"dist_vue/**/*",
			"src/**/*",
			"electron/**/*",
			"!**/node_modules/*",
			"package.json"
		],
		"extraMetadata": {
			"main": "electron/main.js"
		},
		"win": {
			"target": "nsis"
		},
		"mac": {
			"target": "dmg"
		},
		"linux": {
			"target": "AppImage"
		}
	},
	"dependencies": {
		"@iktakahiro/markdown-it-katex": "^4.0.1",
		"@mdit/plugin-img-lazyload": "^0.22.0",
		"@panzoom/panzoom": "^4.6.0",
		"axios": "^1.9.0",
		"core-js": "^3.8.3",
		"dexie": "^4.0.11",
		"flowchart.js": "^1.18.0",
		"highlight.js": "^11.11.1",
		"jspdf": "^3.0.1",
		"katex": "^0.16.22",
		"markdown-it": "^14.1.0",
		"markdown-it-emoji": "^3.0.0",
		"markdown-it-mathjax3": "^4.3.2",
		"markdown-it-task-lists": "^2.1.1",
		"mermaid": "^11.6.0",
		"plantuml-encoder": "^1.4.0",
		"vue": "^3.2.13",
		"vue-i18n": "^11.1.3",
		"vue-router": "^4.0.3",
		"vue-toast-notification": "^3.1.3",
		"vuex": "^4.0.0"
	},
	"devDependencies": {
		"@babel/plugin-transform-class-static-block": "^7.27.1",
		"@vue/cli-plugin-babel": "~5.0.0",
		"@vue/cli-plugin-router": "~5.0.0",
		"@vue/cli-plugin-vuex": "~5.0.0",
		"@vue/cli-service": "~5.0.0",
		"cross-env": "^7.0.3",
		"electron": "^37.1.0",
		"electron-builder": "^26.0.12",
		"less": "^4.0.0",
		"less-loader": "^8.0.0",
		"typescript": "^5.8.3",
		"wait-on": "^8.0.3",
		"webpack": "^5.99.9"
	}
}
